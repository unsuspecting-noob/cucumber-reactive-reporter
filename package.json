{
  "name": "cucumber-reactive-reporter",
  "version": "1.0.0",
  "description": "A filterable html report generator for cucumberjs (react-redux)",
  "private": true,
  "homepage": "./",
  "main": "dist/cucumberjs-reporter.cjs.js",
  "module": "dist/cucumberjs-reporter.esm.js",
  "files": ["react/**/*"],
  "repository": "github.com:unsuspecting-noob/cucumber-reactive-reporter.git",
  "keywords": ["cucumber", "cucumberjs", "html", "report", "reporter"],
  "license": "MIT",
  "scripts": {
    "start": "react-scripts start",
    "build": "rollup -c",
    "install": "npm run -s build",
    "prebuildreact": "rm -rf build",
    "buildreact": "react-scripts build && npm run build",
    "preinstallmodule": "rm -rf dist",
    "installmodule": "npm run buildreact && node prepDist.mjs",
    "postinstallmodule": "node install.mjs",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "ncp": "^2.0.0",
    "rollup": "^1.0.0",
    "rollup-plugin-babel": "^4.2.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-node-resolve": "^4.0.0"
  },
  "devDependencies": {
    "@cucumber/tag-expressions": "^4.1.0",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@fontsource/roboto": "^4.5.2",
    "@monaco-editor/react": "^4.4.5",
    "@mui/icons-material": "^5.2.5",
    "@mui/material": "^5.2.5",
    "@mui/styles": "^5.3.0",
    "@reduxjs/toolkit": "^1.4.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "dayjs": "^1.10.7",
    "fast-memoize": "^2.5.2",
    "font-awesome": "^4.7.0",
    "lodash.memoize": "^4.1.2",
    "moment": "^2.27.0",
    "ncp": "^2.0.0",
    "pretty-data": "^0.40.0",
    "react": "^17.0.2",
    "react-copy-to-clipboard": "^5.0.4",
    "react-dom": "^17.0.2",
    "react-masonry-css": "^1.0.16",
    "react-minimal-pie-chart": "^8.2.0",
    "react-redux": "^7.2.6",
    "react-scripts": "3.4.1",
    "react-select": "^5.2.1",
    "recursive-readdir": "^2.2.2",
    "redux": "^4.0.5",
    "redux-query-sync": "^0.1.10",
    "redux-thunk": "^2.3.0",
    "renamer": "^4.0.0",
    "replace-in-file": "^6.3.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}